{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "no-loops"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-empty-function": 0,
      "@typescript-eslint/no-empty-function": [0],
      "no-loops/no-loops": 2,
      "semi": [2, "always"],
      "no-else-return": [2]
    }
  }